#!/usr/bin/env bash

export VAGRANT_DEFAULT_PROVIDER="aws"

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"

  source "$shome/script/profile"

  if [[ -x "$(which "parity-${1:-}" 2>&- || true)" ]]; then
    exec parity "$@"
  else
    exec "$shome/vendor/virtualenv/bin/aws" "$@"
  fi
}

source sub "$0" "$@"
